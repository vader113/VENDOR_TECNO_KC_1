<EccTable>
    <!--
        Gabon
        The attribute definition for tag EccEntry:
        - Ecc: the emergnecy number
        - Category: the service category
        - Condition: there are following values:
            - 0: ecc only when no sim
            - 1: ecc always
            - 2: MMI will show ecc but send to nw as normal call
    -->
    <EccEntry Ecc="112" Category="0" Condition="1" Name="Emergency Number" />
    <EccEntry Ecc="911" Category="0" Condition="1" Name="Emergency Number" />
    <EccEntry Ecc="177" Category="0" Condition="2" Name="Police" />
    <EccEntry Ecc="18" Category="0" Condition="2" Name="Gabon Fire" />
    <EccEntry Ecc="1300" Category="0" Condition="2" Name="Ambulance" />
    <EccEntry Ecc="1401" Category="0" Condition="2" Name="Denunciation of COVID incivility" />
    <EccEntry Ecc="1442" Category="0" Condition="2" Name="Free Electricity" />
    <EccEntry Ecc="1410" Category="0" Condition="2" Name="Crisis Cell COVID 19" />
    <EccEntry Ecc="1420" Category="0" Condition="2" Name="Free Transport" />
    <EccEntry Ecc="1488" Category="0" Condition="2" Name="Samu Social" />
</EccTable>
